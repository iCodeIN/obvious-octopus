#include <iostream>
#include <vector>

#include "kmeans.hpp"

int main()
{
   // generate 100 random points
   std::vector<std::vector<double>> points;
   for(int i=0;i<10000;i++)
   {
      std::vector<double> a;
      a.push_back((i * 565 + 587) % 10000);
      a.push_back((i * 814 + 161) % 10000);
      points.push_back(a);
   }

   auto clusters = geometry::KMeans::kmeansWithEuclideanMetric(points, 3);

   return 0;
}
